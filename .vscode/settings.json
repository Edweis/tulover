{
 "editor.formatOnSave": true,
 "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.prettier": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "json",
    "yaml"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
